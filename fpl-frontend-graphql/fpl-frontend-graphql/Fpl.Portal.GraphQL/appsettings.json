{
  "CacheProfiles": {
    "StaticFiles": {
      "Duration": 31536000,
      "Location": "Any"
    }
  },
  "GraphQL": {
    "MaxAllowedExecutionDepth": 100,
    "Paging": {
      "AllowBackwardPagination": true,
      "DefaultPageSize": 10,
      "IncludeTotalCount": true,
      "MaxPageSize": 100,
      "RequirePagingBoundaries": true
    },
    "Request": {
      "ExecutionTimeout": "00:05:00",
      "IncludeExceptionDetails": false,
      "QueryCacheSize": 100,
      "TracingPreference": "OnDemand",
      "Complexity": {
        "Enable": true,
        "ApplyDefaults": true,
        "DefaultComplexity": 1,
        "DefaultResolverComplexity": 5,
        "MaximumAllowed": 300
      }
    }
  },
  "Kestrel": {
    "Limits": {
      "MaxRequestBodySize": 20971520,
      "MaxRequestHeaderCount": 40
    }
  },
  "FplApi": {
    "EndPoints": {
      "BootstrapStatic": "bootstrap-static",
      "GetFixtures": "fixtures"
    }
  },
  "ApiFunction": {
    "ApiBaseUrl": "https://fantasy.premierleague.com/api/"
  },
  "Compression": {
    "MimeTypes": []
  },
  "ForwardedHeaders": {
    "ForwardedHeaders": "XForwardedFor,XForwardedHost,XForwardedProto",
    "AllowedHosts": [
      "*"
    ]
  }
}